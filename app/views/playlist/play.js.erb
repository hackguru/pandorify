<% if @page == 1 %>
$('#song_view').html("<%= escape_javascript(render 'song/songs', :songs => @songs, :type =>@type, :appended => false) %>");
<% else %>
$('#song_view').children(":first").append("<%= escape_javascript(render 'song/songs', :songs => @songs, :type =>@type, :appended => true) %>");
<% end %>

$('#song_view').attr("type","<%=@type%>");
$('#song_view').attr("content",'playlist');
$('#song_view').attr("content_id","<%=@playlist.id%>");
$('body').removeClass('loading');
$(".slide-out-div").css("display","none");